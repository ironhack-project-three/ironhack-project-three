{
  "name": "project2-template",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "client"
  ],
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "node ./bin/www",
    "dev:all": "concurrently \"cross-env PORT=3000 nodemon -i ./client ./bin/www\" \"cross-env PORT=5000 yarn workspace client run start\"",
    "dev:front": "cross-env PORT=5000 yarn workspace client run start",
    "dev:back": "cross-env PORT=3000 nodemon -i ./client ./bin/www",
    "build": "yarn workspace client run build",
    "test:client": "yarn workspace client run test",
    "test": "cross-env NODE_ENV=test jest"
  },
  "dependencies": {
    "axios": "^0.22",
    "bcrypt": "^5",
    "cloudinary": "^1",
    "connect-mongo": "^4",
    "cookie-parser": "^1",
    "cors": "^2.8.5",
    "dotenv": "^10",
    "eslint": "^7.32.0",
    "express": "^4",
    "express-jwt": "^6.1.0",
    "express-session": "^1",
    "hbs": "^4",
    "http-errors": "^1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.1.3",
    "mongoose": "^6",
    "mongoose-paginate-v2": "^1.4.2",
    "morgan": "^1",
    "multer": "^1",
    "multer-storage-cloudinary": "^4",
    "prettier": "^2.4.1",
    "serve-favicon": "^2",
    "stripe": "^8.178.0",
    "typescript": "^4.4.3",
    "webpack": "^5.58.0"
  },
  "devDependencies": {
    "concurrently": "^6",
    "cross-env": "^7.0.3",
    "debug": "^4.3.2",
    "jest": "^27.3.1",
    "nodemon": "^2",
    "supertest": "^6.1.6"
  },
  "packageManager": "yarn@3.0.2",
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/client/"
    ]
  }
}
